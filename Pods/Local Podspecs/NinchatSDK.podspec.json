{
  "name": "NinchatSDK",
  "version": "0.0.39",
  "summary": "iOS SDK for Ninchat.",
  "description": "For building iOS applications using Ninchat messaging.",
  "homepage": "https://ninchat.com/",
  "license": {
    "type": "Ninchat",
    "file": "LICENSE.md"
  },
  "authors": {
    "Matti Dahlbom": "matti.dahlbom@qvik.fi"
  },
  "source": {
    "git": "https://github.com/somia/ninchat-sdk-ios.git",
    "tag": "0.0.39"
  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "SDK",
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS[arch=i386]": "-Wl,-read_only_relocs,suppress -lstdc++",
    "ENABLE_BITCODE": "NO"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "static_framework": true,
  "dependencies": {
    "AFNetworking": [
      "~> 3.0"
    ],
    "NinchatLowLevelClient": [
      "~> 0"
    ],
    "GoogleWebRTC": [
      "~> 1.1"
    ]
  },
  "module_name": "NinchatSDK",
  "requires_arc": true,
  "subspecs": [
    {
      "name": "SDK",
      "source_files": "NinchatSDK/**/*.{h,m}",
      "public_header_files": "NinchatSDK/**/*.h",
      "resource_bundles": {
        "NinchatSDKUI": [
          "NinchatSDK/**/*.{storyboard,xib,xcassets,strings}"
        ]
      }
    }
  ]
}
